      <Button fx:id="stopBtn" layoutX="72.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleStopBtnAction" text="Stop" />
      <ListView fx:id="logView" layoutX="13.0" layoutY="47.0" prefHeight="344.0" prefWidth="578.0" />
